{
  "uid": "fileflow-core-metrics",
  "title": "FileFlow - Core Metrics",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "tags": ["fileflow", "prod", "core"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": { "text": "AMP-prod", "value": "AMP-prod" }
      },
      {
        "name": "job",
        "type": "query",
        "datasource": "$datasource",
        "query": "label_values(up, job)",
        "current": { "text": "fileflow-metrics", "value": "fileflow-metrics" }
      },
      {
        "name": "application",
        "type": "query",
        "datasource": "$datasource",
        "query": "label_values(jvm_memory_used_bytes, application)",
        "regex": "/fileflow.*/",
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "task_id",
        "label": "ECS Task ID",
        "type": "query",
        "datasource": "$datasource",
        "query": "label_values(up{job=\"$job\"}, aws_ecs_task_id)",
        "regex": "/.+/",
        "includeAll": true,
        "multi": true,
        "refresh": 2
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "üìä Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Instances UP",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(up{job=\"$job\"})",
          "legendFormat": "UP"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 2 }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total RPS",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\"}[1m]))",
          "legendFormat": "RPS"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "decimals": 1 }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Avg Latency",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_sum{job=\"$job\"}[1m])) / sum(rate(http_server_requests_seconds_count{job=\"$job\"}[1m]))",
          "legendFormat": "Avg"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "P99 Latency",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"$job\"}[1m])))",
          "legendFormat": "P99"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Error Rate %",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\",status=~\"5..\"}[1m])) / sum(rate(http_server_requests_seconds_count{job=\"$job\"}[1m])) * 100 or vector(0)",
          "legendFormat": "Error %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Heap Usage %",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "avg(jvm_memory_used_bytes{job=\"$job\",area=\"heap\"} / jvm_memory_max_bytes{job=\"$job\",area=\"heap\"}) * 100",
          "legendFormat": "Heap %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      }
    },

    {
      "id": 200,
      "type": "row",
      "title": "üöÄ Traffic & Latency",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Request Rate (RPS)",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\"}[1m]))",
          "legendFormat": "Total RPS"
        },
        {
          "expr": "sum by (aws_ecs_task_id) (rate(http_server_requests_seconds_count{job=\"$job\"}[1m]))",
          "legendFormat": "{{aws_ecs_task_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Response Time (P50 / P95 / P99)",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"$job\"}[1m])))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"$job\"}[1m])))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"$job\"}[1m])))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },

    {
      "id": 300,
      "type": "row",
      "title": "üìÅ File Operations (SQS Processing)",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 20,
      "type": "stat",
      "title": "File Processing - Success Rate",
      "gridPos": { "x": 0, "y": 15, "w": 6, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(file_processing_success_total{job=\"$job\"}[5m])) / sum(rate(file_processing_total{job=\"$job\"}[5m])) * 100 or vector(100)",
          "legendFormat": "Success %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      }
    },
    {
      "id": 21,
      "type": "stat",
      "title": "External Download - Success Rate",
      "gridPos": { "x": 6, "y": 15, "w": 6, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(external_download_success_total{job=\"$job\"}[5m])) / sum(rate(external_download_total{job=\"$job\"}[5m])) * 100 or vector(100)",
          "legendFormat": "Success %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      }
    },
    {
      "id": 22,
      "type": "stat",
      "title": "DLQ Messages (5m)",
      "gridPos": { "x": 12, "y": 15, "w": 6, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(increase(file_processing_dlq_total{job=\"$job\"}[5m])) + sum(increase(external_download_dlq_total{job=\"$job\"}[5m])) or vector(0)",
          "legendFormat": "DLQ"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "id": 23,
      "type": "stat",
      "title": "Lock Skipped (5m)",
      "gridPos": { "x": 18, "y": 15, "w": 6, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(increase(external_download_lock_skipped_total{job=\"$job\"}[5m])) or vector(0)",
          "legendFormat": "Skipped"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "orange", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "id": 24,
      "type": "timeseries",
      "title": "File Processing Rate",
      "gridPos": { "x": 0, "y": 19, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(file_processing_total{job=\"$job\"}[1m]))",
          "legendFormat": "Total"
        },
        {
          "expr": "sum(rate(file_processing_success_total{job=\"$job\"}[1m]))",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(file_processing_failure_total{job=\"$job\"}[1m]))",
          "legendFormat": "Failure"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Success" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Failure" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 25,
      "type": "timeseries",
      "title": "External Download Rate",
      "gridPos": { "x": 12, "y": 19, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(external_download_total{job=\"$job\"}[1m]))",
          "legendFormat": "Total"
        },
        {
          "expr": "sum(rate(external_download_success_total{job=\"$job\"}[1m]))",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(external_download_failure_total{job=\"$job\"}[1m]))",
          "legendFormat": "Failure"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Success" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Failure" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 26,
      "type": "timeseries",
      "title": "File Processing Duration (P50/P95/P99)",
      "gridPos": { "x": 0, "y": 27, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(file_processing_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(file_processing_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(file_processing_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "id": 27,
      "type": "timeseries",
      "title": "External Download Duration (P50/P95/P99)",
      "gridPos": { "x": 12, "y": 27, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(external_download_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(external_download_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(external_download_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },

    {
      "id": 400,
      "type": "row",
      "title": "‚ö†Ô∏è Errors & Status Codes",
      "gridPos": { "x": 0, "y": 35, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 30,
      "type": "timeseries",
      "title": "HTTP Status Code Distribution",
      "gridPos": { "x": 0, "y": 36, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\",status=~\"2..\"}[1m]))",
          "legendFormat": "2xx Success"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\",status=~\"4..\"}[1m]))",
          "legendFormat": "4xx Client Error"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"$job\",status=~\"5..\"}[1m]))",
          "legendFormat": "5xx Server Error"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "2xx Success" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "4xx Client Error" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "5xx Server Error" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 31,
      "type": "timeseries",
      "title": "DLQ Incoming Rate",
      "gridPos": { "x": 12, "y": 36, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(file_processing_dlq_total{job=\"$job\"}[1m]))",
          "legendFormat": "File Processing DLQ"
        },
        {
          "expr": "sum(rate(external_download_dlq_total{job=\"$job\"}[1m]))",
          "legendFormat": "External Download DLQ"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 20 }
        }
      }
    },

    {
      "id": 500,
      "type": "row",
      "title": "üóÑÔ∏è Database (HikariCP)",
      "gridPos": { "x": 0, "y": 44, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 40,
      "type": "timeseries",
      "title": "Connection Pool Usage",
      "gridPos": { "x": 0, "y": 45, "w": 8, "h": 6 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "hikaricp_connections_active{job=\"$job\"}",
          "legendFormat": "Active"
        },
        {
          "expr": "hikaricp_connections_idle{job=\"$job\"}",
          "legendFormat": "Idle"
        },
        {
          "expr": "hikaricp_connections_pending{job=\"$job\"}",
          "legendFormat": "Pending"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" }
      }
    },
    {
      "id": 41,
      "type": "gauge",
      "title": "Pool Utilization %",
      "gridPos": { "x": 8, "y": 45, "w": 8, "h": 6 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "avg(hikaricp_connections_active{job=\"$job\"} / hikaricp_connections_max{job=\"$job\"}) * 100",
          "legendFormat": "Usage %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 80 }
            ]
          }
        }
      }
    },
    {
      "id": 42,
      "type": "timeseries",
      "title": "Connection Acquire Time",
      "gridPos": { "x": 16, "y": 45, "w": 8, "h": 6 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "rate(hikaricp_connections_acquire_seconds_sum{job=\"$job\"}[1m]) / rate(hikaricp_connections_acquire_seconds_count{job=\"$job\"}[1m])",
          "legendFormat": "Avg Acquire Time"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },

    {
      "id": 600,
      "type": "row",
      "title": "üìÖ Scheduler Jobs",
      "gridPos": { "x": 0, "y": 51, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 50,
      "type": "timeseries",
      "title": "Scheduler Job Executions",
      "gridPos": { "x": 0, "y": 52, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum by (job_name) (rate(scheduler_job_runs_total{job=\"$job\"}[5m]))",
          "legendFormat": "{{job_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" }
      }
    },
    {
      "id": 51,
      "type": "timeseries",
      "title": "Scheduler Job Duration",
      "gridPos": { "x": 12, "y": 52, "w": 12, "h": 8 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, job_name) (rate(scheduler_job_duration_seconds_bucket{job=\"$job\"}[5m])))",
          "legendFormat": "{{job_name}} P95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right" }
      }
    },
    {
      "id": 52,
      "type": "stat",
      "title": "Scheduler Success Rate",
      "gridPos": { "x": 0, "y": 60, "w": 12, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
          "expr": "sum(rate(scheduler_job_success_total{job=\"$job\"}[5m])) / sum(rate(scheduler_job_runs_total{job=\"$job\"}[5m])) * 100 or vector(100)",
          "legendFormat": "Success %"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"] },
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      }
    },
    {
      "id": 53,
      "type": "timeseries",
      "title": "Scheduler Items Processed",
      "gridPos": { "x": 12, "y": 60, "w": 12, "h": 4 },
      "datasource": "$datasource",
      "targets": [
        {
